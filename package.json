{
  "name": "jest-jasmine-reporters-example",
  "version": "1.0.0",
  "description": "Example project demonstrating how to implement jasmine-reporters with Jest",
  "main": "server.js",
  "scripts": {
    "prestart": "npm run test",
    "start": "node server",
    "compile": "browserify -t [ babelify --presets [ react ] ] client/main.js -o client/compiled.js",
    "test": "jest --coverage",
    "posttest": "npm run compile"
  },
  "keywords": [
    "Jest",
    "Jasmine",
    "Test",
    "Coverage",
    "CI"
  ],
  "author": "Joe Devine",
  "license": "ISC",
  "dependencies": {
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "express": "^4.13.4",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "jest": {
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "./setup-jasmine-env.js",
    "collectCoverageOnlyFrom": {
      "./client/components/content.js": true
    },
    "unmockedModulePathPatterns": [
      "./node_modules/react",
      "./node_modules/react-addons-test-utils",
      "./node_modules/jasmine-reporters"
    ]
  },
  "devDependencies": {
    "babel-jest": "^9.0.0",
    "babel-preset-es2015": "^6.9.0",
    "browserify": "^13.0.1",
    "jasmine-reporters": "^2.1.1",
    "jest-cli": "^12.1.1",
    "react-addons-test-utils": "^15.1.0"
  }
}
